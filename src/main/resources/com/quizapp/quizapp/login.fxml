<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quizapp.quizapp.LoginController">
   <children>
      <Label alignment="CENTER" layoutX="284.0" layoutY="31.0" text="Login">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="168.0" layoutY="87.0" text="Email" />
      <TextField fx:id="loginEmailTextField" layoutX="168.0" layoutY="111.0" prefHeight="25.0" prefWidth="275.0" promptText="abc@xyz.com" />
      <Label layoutX="169.0" layoutY="155.0" text="Password" />
      <PasswordField fx:id="loginPasswordField" layoutX="168.0" layoutY="178.0" prefHeight="25.0" prefWidth="275.0" promptText="***" />
      <Label layoutX="169.0" layoutY="264.0" text="You done have an account?" />
      <Button layoutX="167.0" layoutY="220.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="25.0" prefWidth="275.0" text="Login" />
      <Button layoutX="168.0" layoutY="290.0" mnemonicParsing="false" onAction="#goToRegister" text="Register" />
   </children>
</AnchorPane>
